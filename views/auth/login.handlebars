<div class="col-6 m-auto">
    <form class="form-signin text-center" method="POST" action="/login">
        {{!-- <img class="mb-4" src="/images/logo_celke.png" alt="Celke" width="72" height="72"> --}}
        <h1 class="h3 mb-3 font-weight-normal">Área Restrita</h1>
        {{>_msg}}
        <div class="form-group mb-3">
            <label>Usuário</label>
            <input type="text" name="email" class="form-control col-6 mt-3" placeholder="Digite o usuário">
        </div>
        <div class="form-group">
            <label>Senha</label>
            <input type="password" name="password" class="form-control mt-3" placeholder="Digite a senha">
        </div>
        <button class="btn btn-lg btn-primary btn-block mt-3" type="submit">Acessar</button>
    </form>
</div>